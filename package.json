{
  "name": "virgil-pythia",
  "version": "0.2.3",
  "description": "Virgil Pythia Service Client",
  "main": "dist/virgil-pythia.cjs.js",
  "module": "dist/virgil-pythia.es.js",
  "browser": {
    "./dist/virgil-pythia.cjs.js": "./dist/virgil-pythia.browser.cjs.js",
    "./dist/virgil-pythia.es.js": "./dist/virgil-pythia.browser.es.js",
    "./src/tests/utils/getRandomBytes": "./src/tests/utils/getRandomBytes-browser.js"
  },
  "scripts": {
    "rollup:cjs": "rollup -c config/rollup.config.cjs.js && rollup -c config/rollup.config.browser.cjs.js",
    "rollup:es": "rollup -c config/rollup.config.es.js && rollup -c config/rollup.config.browser.es.js",
    "rollup:umd": "rollup -c config/rollup.config.umd.js",
    "rollup": "npm-run-all --parallel rollup:*",
    "clean": "rimraf dist",
    "minify": "uglifyjs dist/virgil-pythia.browser.umd.js -o dist/virgil-pythia.browser.umd.min.js -c -m",
    "build": "npm-run-all clean rollup minify",
    "test:node": "BABEL_ENV=test mocha --require babel-register --require dotenv/config --require chai/register-assert \"src/tests/**/*.test.js\"",
    "test:browser": "karma start",
    "test": "npm-run-all test:*",
    "preversion": "npm run test",
    "version": "npm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VirgilSecurity/pythia-node.git"
  },
  "keywords": [
    "pythia"
  ],
  "author": "Virgil Security Inc. <support@virgilsecurity.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/VirgilSecurity/pythia-node/issues"
  },
  "homepage": "https://github.com/VirgilSecurity/pythia-node#readme",
  "dependencies": {
    "cross-fetch": "^2.2.2"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "buffer-es6": "^4.9.3",
    "builtin-modules": "^3.0.0",
    "chai": "^4.1.2",
    "dotenv": "^5.0.1",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "karma": "^3.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-rollup-preprocessor": "^6.0.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.60.7",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-inject": "^2.0.0",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "sinon": "^4.5.0",
    "uglify-js": "^3.4.5",
    "virgil-crypto": "^3.0.0",
    "virgil-sdk": "^5.0.0"
  },
  "peerDependencies": {
    "virgil-crypto": "^3.0.0",
    "virgil-sdk": "^5.0.0"
  }
}
